<?xml version="1.0"?>
<!-- AIM 120 based on AIM-9 by xii Richard Harrison, 29-76-2015 -->
<PropertyList>
    <path>aim-120.ac</path>

    <animation>
        <type>select</type>
        <object-name>aim-120</object-name>
        <condition>
            <property>sim/model/f15/systems/armament/aim120/flags/msl-id-7</property>
        </condition>
    </animation>

    <model>
        <path>Models/Stores/aim-120/motor-smoke-7.xml</path>
        <offsets>
            <x-m>3</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </offsets>
    </model>

    <model>
        <path>Models/Stores/aim-120/explosion-7.xml</path>
        <offsets>
            <x-m>3</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </offsets>
    </model>

</PropertyList>
