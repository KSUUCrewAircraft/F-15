    <!-- lighting -->

    <animation>
        <object-name>instrument</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
            <not>
                <greater-than>
                    <property>controls/lighting/panel</property>
                    <property>controls/lighting/eng-inst</property>
                </greater-than>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/eng-inst</factor-prop>
            <red>  0.1000</red>
            <green>0.0576</green>
            <blue> 0.0161</blue>
            <red-prop>controls/lighting/instrument-red</red-prop>
            <green-prop>controls/lighting/instrument-green</green-prop>
            <blue-prop>controls/lighting/instrument-blue</blue-prop>
        </emission>
    </animation>

    <animation>
        <object-name>instrument</object-name>
        <object-name>mount</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/panel-norm</factor-prop>
            <red>  1.000</red>
            <green>0.576</green>
            <blue> 0.161</blue>
            <red-prop>controls/lighting/panel-red</red-prop>
            <green-prop>controls/lighting/panel-green</green-prop>
            <blue-prop>controls/lighting/panel-blue</blue-prop>

        </emission>
    </animation>
    <!-- implicit ALS lighting see http://wiki.flightgear.org/ALS_technical_notes -->
    <effect>
        <name>f15panel-left</name>
        <inherits-from>Effects/f15panel-right</inherits-from>
        <object-name>hyds-mount</object-name>
    </effect>
    <effect>
        <name>f15interior</name>
        <inherits-from>Effects/f15interior</inherits-from>
        <object-name>mount</object-name>
        <object-name>instrument</object-name>
    </effect>
</PropertyList>
