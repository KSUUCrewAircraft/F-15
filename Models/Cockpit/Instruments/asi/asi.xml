    <!-- Air Speed Indicator's Bug Setting -->
    <animation>
        <type>knob</type>
        <object-name>knob</object-name>
        <property>sim/model/f15/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>
        <factor>0</factor>
        <center>
            <x-m>0.0197</x-m>
            <y-m>0.0336</y-m>
            <z-m>-0.0323</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/f15/instrumentation/airspeed-indicator/safe-speed-limit-bug</property>

                <factor>10</factor>
                <min>0</min>
                <max>1000</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>


    <!-- Lightning -->
    <animation>
        <object-name>mount</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/panel-norm</factor-prop>
            <red>  1.000</red>
            <green>0.576</green>
            <blue> 0.161</blue>
            <red-prop>controls/lighting/panel-red</red-prop>
            <green-prop>controls/lighting/panel-green</green-prop>
            <blue-prop>controls/lighting/panel-blue</blue-prop>

        </emission>
    </animation>

    <animation>
        <object-name>instrument</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/flt-inst</factor-prop>
            <red>  0.1000</red>
            <green>0.0576</green>
            <blue> 0.0161</blue>
            <red-prop>controls/lighting/instrument-red</red-prop>
            <green-prop>controls/lighting/instrument-green</green-prop>
            <blue-prop>controls/lighting/instrument-blue</blue-prop>

        </emission>
    </animation>
<!-- ALS Model Interior (Shadow) -->
    <effect>
        <name>f15interior</name>
        <inherits-from>../Effects/f15interior</inherits-from>
        <object-name>mount</object-name>
        <object-name>instrument</object-name>
    </effect>
</PropertyList>
